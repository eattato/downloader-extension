{
  "manifest_version": 3,
  "name": "Split Downloader",
  "description": "Split files and download fast using threading",
  "version": "1.1",
  "icons": {
    "128": "128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["downloads", "declarativeContent", "notifications", "tabs"],
  "action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}
